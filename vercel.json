{
      "rewrites": [
            {
                  "source": "/api/v1/:path*",
                  "destination": "https://perf-be.onrender.com/api/v1/:path*"
            },
            {
                  "source": "/(.*)",
                  "destination": "/index.html"
            }
      ],
      "headers": [
            {
                  "source": "/api/v1/:path*",
                  "headers": [
                        {
                              "key": "Cache-Control",
                              "value": "no-store, must-revalidate"
                        }
                  ]
            }
      ]
}
